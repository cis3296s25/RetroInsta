NODE_ENV=test

# Backend - Match the port if you changed it in docker-compose.test.yml
SERVER_HOST=0.0.0.0
SERVER_PORT=7006 # Or 7005 if you didn't change it
BACKEND_URL=http://backend-test:7006 # Internal URL for services

# MongoDB - Match docker-compose.test.yml
MONGO_HOST=mongo-test
MONGO_PORT=27017
MONGO_DB=testdb # MUST match the test DB name
USERS_COLLECTION=users # Keep collection names or change if needed for tests
POSTS_COLLECTION=posts
COMMENTS_COLLECTION=comments

# MinIO - Match docker-compose.test.yml
MINIO_ENDPOINT=minio-test
MINIO_PORT=9000
MINIO_ACCESS_KEY=minio_test_access_key
MINIO_SECRET_KEY=minio_test_secret_key
MINIO_BUCKET=testbucket # MUST match the test bucket name
# MINIO_PUBLIC_HOST=localhost:9002 # If accessing Minio from host outside Docker

# Other variables needed by your app
GOOGLE_CLIENT_ID=test_google_client_id # Use dummy values if not testing auth directly yet